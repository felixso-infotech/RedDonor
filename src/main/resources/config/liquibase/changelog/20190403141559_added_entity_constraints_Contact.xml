<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Contact.
    -->
    <changeSet id="20190403141559-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="address_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"/>

        <addForeignKeyConstraint baseColumnNames="blood_group_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact_blood_group_id"
                                 referencedColumnNames="id"
                                 referencedTableName="blood_group"/>

        <addForeignKeyConstraint baseColumnNames="contact_id"
                                 baseTableName="contact_contact_set"
                                 constraintName="fk_contact_contact_set_contact_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact"/>
        <addForeignKeyConstraint baseColumnNames="contact_set_id"
                                 baseTableName="contact_contact_set"
                                 constraintName="fk_contact_contact_set_contact_set_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact"/>
        
    </changeSet>
</databaseChangeLog>
